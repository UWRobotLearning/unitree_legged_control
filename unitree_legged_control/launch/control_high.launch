
<!--
   Record data in following bags:

   - highdim (IMU, cameras, ..)

   - lowdim (force sensors, pose)

  -->

<launch>
    <arg name="ctrl_level" default="highlevel"/>

    <node pkg="unitree_legged_control" type="A1_control" name="node_A1_control" output="screen" args="$(arg ctrl_level)"/>

   <!--<node pkg = "rosbag" type = "record" name = "a1bag" args = "-O /root/src/unitree_legged_control/unitree_legged_control/launch/bagfiles/a1bag.bag low_state/imu"/>-->

    <param name="control_level" value="$(arg ctrl_level)"/>
</launch>

